let athleteFinalPosition = 'first place';

switch (athleteFinalPosition) {
  case '1st place':
    console.log('1st place recieves a GOLD medal');
    break;
  case '2nd place':
    console.log('2nd place recieves a SILVER medal');
    break;
  case '3rd place';
    console.log('3rd place recieves a BRONZE medal');
    break;
  default:
    console.log('Invalid item');
    break;
    }
